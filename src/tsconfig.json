{
	"compilerOptions": {
		"target"             : "es6",
        "module"             : "commonjs",
		"sourceMap"          : true,
		"inlineSources"      : false,
		"noImplicitAny"      : false,
		"removeComments"     : true,
		"preserveConstEnums" : false,
        
        "moduleResolution"   : "node",
        "allowUnreachableCode": true
	}
}